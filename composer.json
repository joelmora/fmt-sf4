{
    "name": "isoft/fmt-sf4",
    "type": "library",
    "description": "Codigo Fuente Comun para implementar en Microservicios",
    "keywords": [
        "isoft",
        "fmt-sf4"
    ],
    "homepage": "https://github.com/joelmora/fmt-sf4",
    "license": "MIT",
    "authors": [
        {
            "name": "Nelsonmora",
            "email": "nelsonmora48@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        // "php" : "~7.1.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "isoft\\fmt-sf4\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "isoft\\fmt-sf4\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
